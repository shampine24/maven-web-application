pipeline {
    agent any
    tools{
        maven "maven3.8.6"
    }
    stages{
        stage("checkout"){
            steps{
                git branch:"master",credentialsId:"www",url:"https://github.com/shampine24/maven-web-application.git"
            }
        }
        stage("package"){
            steps{
                sh "mvn clean package"
            }
        }
    }
}